<?php

/**
 * Implements hook_views_api().
 */
function fieramilanomedia_views_views_api() {
    return array(
        'api' => '3.0',
    );
}

/**
 * Implements hook_views_default_views().
 */
function fieramilanomedia_views_views_default_views() {
    $path = './' . drupal_get_path('module', 'realestate_views') . '/views/*.inc';
    $views = array();
    foreach (glob($path) as $views_filename) {
        require_once($views_filename);
    }
    return $views;
}

drupal_add_css(drupal_get_path('module', 'realestate_views') .'/realestate_views.css');
